{
  "name": "web-tamper",
  "version": "0.0.1",
  "manifest_version": 2,
  "icons": {
    "16": "icons/icon-16.png",
    "128": "icons/icon-128.png"
  },
  "default_locale": "en",
  "background": {
    "scripts": [
      "scripts/background.js"
    ]
  },
  "permissions": [
    "tabs",
    "storage",
    "http://*/*",
    "https://*/*"
  ],
  "options_ui": {
    "page": "options.html"
  },
    "default_title": "Web Tamper",
    "default_popup": "popup.html"
  }
}
